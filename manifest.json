{
  "manifest_version": 2,

  "name": "Phish On This Day",
  "description": "This extension shows Phish events on this day in History.",
  "version": "1.0",
  "background": {
    "scripts": ["scrape.js"]
  },
  "browser_action": {
    "default_icon": "phish.png",
    "default_popup": "popup.html",
    "default_title": "Phish On This Day"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ]
}
